n = int(input("Enter an integer:"))

if n > 0:
    for a in range(1, n+n):
        print(a*a/(n/2))
else:
    print("Now quitting")
# (a)
i = 100

while i > 0:
    print(i)
    i = i - 1

# (b)
num = 12345

while num > 0:
    print(num % 10)
    num = num // 10

# (c)
count = 0
sum = 0

while count < 10:
    sum += num
    num = num + 1

    if count == 10:
        print(sum/float(count))
    break

# (a)
min = 0
max = num

while num < 0:
    min = num

# (b)
sum = 0

while min <= max:
    sum += min
    min += 1
