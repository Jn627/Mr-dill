weather = 'raining'

if weather == 'sunny':
    print("wear sunblock")
elif weather == 'snow':
    print("going skiing")
else:
    print(weather)
n = int(input("Enter an integer: "))

if n < 1:
    print("invalid value")
else:
    for i in range(1, n+1):
        print(i*i)

if int('zero') == 0:
    print("zero")
elif str(0) == 'zero':
    print(0)
elif str(0) == '0':
    print(str(0))
else:
    print("none of the above")
