print(counts2)
#to ass a key value pair
counts['games']
print(counts)
counts['players']
print(counts)
#to find num items
print(len(counts))
#to check if a key is in a dictionary
if 'players' in counts:
    print('players found')
if'managers' not in counts:
    print('managers not found')
#To print a value
print(counts['games'])
print(counts)
